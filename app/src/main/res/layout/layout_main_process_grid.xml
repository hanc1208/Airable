<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <ImageView
            android:id="@+id/main_process_grid_image"
            android:layout_width="57dp"
            android:layout_height="57dp"
            android:src="@drawable/airport_1"
            android:layout_marginBottom="5dp"/>
        <TextView
            android:id="@+id/main_process_grid_time"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_gravity="center_horizontal"
            android:text="7분" />
        <LinearLayout
            android:layout_width="57dp"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/main_process_grid_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:text="공항 도착"/>
        </LinearLayout>

        
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_margin="3dp">
        <ImageView
            android:id="@+id/main_process_grid_arrow"
            android:layout_width="wrap_content"
            android:layout_height="57dp"
            android:layout_gravity="center_horizontal"
            android:src="@drawable/flow_1" />
        <TextView
            android:id="@+id/main_process_grid_movingtime"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:text="5분" />
        <View
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
        
    </LinearLayout>

</LinearLayout>
